<strong><h1>Books</h1></strong>
<% @books.each do |book| %>
  <p>Title</p>
  <span><%= book.title %></span>
  <p>body</p>
  <span><%= book.body %></span>

<% end %>

<strong><h2>New book</h2></strong>
<%= form_with model: @books, url:'/books', local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
   <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>